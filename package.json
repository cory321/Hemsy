{
  "name": "hemsy",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "Mobile-first PWA for seamstresses and tailoring businesses",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "dev:host": "next dev --turbo -p 3001 -H ::",
    "build": "next build",
    "build:clean": "rm -rf .next && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "icons:build": "tsx scripts/build-icons.ts",
    "prebuild": "npm run icons:build",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles --forceExit",
    "test:e2e": "playwright test --config config/playwright.config.ts",
    "test:e2e:ui": "playwright test --config config/playwright.config.ts --ui",
    "test:a11y": "playwright test --config config/playwright.config.ts --grep=\"@accessibility\"",
    "test:debug": "jest --detectOpenHandles --verbose --no-cache --runInBand",
    "test:supabase": "tsx src/scripts/test-supabase-connection.ts",
    "test:supabase:write": "tsx src/scripts/test-supabase-write.ts",
    "test:email": "tsx scripts/test-email-config.ts",
    "test:email:api": "tsx scripts/test-email-api.ts",
    "test:email:service": "tsx scripts/test-email-service.ts",
    "svgs:crop": "tsx scripts/crop-svgs.ts",
    "svgs:crop:presets": "tsx scripts/crop-svgs.ts public/presets/garments",
    "setup:database": "tsx src/scripts/setup-database.ts",
    "migrate": "tsx src/scripts/run-migration.ts",
    "migrate:specific": "tsx src/scripts/run-migration.ts",
    "prepare": "husky install",
    "test:stripe-webhook": "node scripts/stripe-webhook-local-test.mjs"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.3.1",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@hookform/resolvers": "^3.10.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/material-nextjs": "^6.5.0",
    "@mui/x-date-pickers": "^7.22.2",
    "@sentry/nextjs": "^8.42.0",
    "@stripe/react-stripe-js": "^3.9.0",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.0",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-query-devtools": "^5.84.1",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.3.1",
    "canvas-confetti": "1.9.3",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.23.12",
    "libphonenumber-js": "^1.12.13",
    "lucide-react": "^0.460.0",
    "next": "^15.1.0",
    "next-cloudinary": "^6.16.0",
    "react": "19.0.0",
    "react-colorful": "^5.6.1",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.5.2",
    "react-swipeable": "^7.0.2",
    "resend": "^4.0.1",
    "stripe": "^17.3.1",
    "tailwind-merge": "^3.3.1",
    "twilio": "^5.3.5",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@clerk/testing": "^1.12.1",
    "@eslint/eslintrc": "^3.2.0",
    "@iconify/json": "^2.2.373",
    "@iconify/tools": "^4.1.2",
    "@iconify/utils": "^3.0.1",
    "@playwright/test": "^1.48.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@types/sharp": "^0.31.1",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "axe-core": "^4.10.2",
    "cheerio": "^1.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.15.0",
    "eslint-config-next": "^15.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "glob": "^11.0.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sharp": "^0.34.3",
    "svg-path-bounds": "^1.0.2",
    "tsx": "^4.20.3",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --config config/.prettierrc --write"
    ],
    "*.{json,md,css}": [
      "prettier --config config/.prettierrc --write"
    ]
  }
}
